  <h2 class="text-center text-[var(--white)] p:w-[48%] font-black p:text-[38px] t:text-[42px] m:text-[28px] t:py-[50px] m:py-[20px]">About us<br/>關於我們</h2>
  <%- require('img.ejs')({
    path: require('@imgs/service/firstclass_fitness_indoor.jpg'),
    alt : '室內環境',
    style: {
      main: 'p:w-full p:h-full t:hidden m:w-[375px] m:h-[180px]',
